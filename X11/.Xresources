! +----------------------------------------------+
! |----->         Font configs             <-----|
! +----------------------------------------------+
Xft.hinting: 1
Xft.rgba: rgb
Xft.hintStyle: "hintsmedium"
Xft.antialias: 1

! +----------------------------------------------+
! |----->           Rofi setup             <-----|
! +----------------------------------------------+
rofi.font:             Monaco 13
rofi.scrollbar-width:  0
rofi.padding:          28
rofi.lines:            5
rofi.color-window:	#2f343f, #2f343f, #009090
rofi.color-normal:	#2f343f, #88c0d0, #2f343f, #009090, #4b5262
rofi.color-active:	#2f343f, #76aebe, #2f343f, #009090, #76aebe
rofi.color-urgent:	#2f343f, #bf616a, #2f343f, #009090, #bf616a

URxvt.cursorBlink  :0
URxvt.url-launcher :usr/bin/firefox-nightly
URxvt.font         :xft:RobotoMono:size=12
URxvt.boldFont     :xft:RobotoMono:bold:size=12

URxvt.lineSpace               : 0
URxvt.letterSpace             : -1
URxvt.scrollBar               : false
URxvt.internalBorder          : 12
URxvt.cursorUnderline         : true
URxvt.geometry                : 60x25
URxvt.thickness               : 5
URxvt.titleBar                : false
URxvt.perl-ext-common         : default,tabbedex,matcher
URxvt.tabbed.new-button       : true
URXvt.tabbed.reopen-on-close  : no
URxvt.keysym.Control-Shift-t  : perl:tabbedex:new_tab
URxvt.keysym.Shifft-Right     : perl:tabbedex:next_tab
URxvt.keysym.Shift-Left       : perl:tabbedex:prev_tab
URxvt.tabbed.no-tabbedex-keys : true
